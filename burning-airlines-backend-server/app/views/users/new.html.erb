<h1>New User</h1>
<%= form_with model: @user, local: true, html: { multipart: true } do |f| %>

  <br>
<%= f.label :name %>
  <br>
  <%= f.text_field :name %>
<br>
  <%= f.label :email %>
  <br>
  <%= f.text_field :email %>
  <br>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br>
  <br>
<%= f.submit %>

<%= link_to 'Back', users_path %>
<% end %>